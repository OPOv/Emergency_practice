<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lg.emergency">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_image"
        android:label="위급이"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Splashscreen"
            android:noHistory="true">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


            </intent-filter>
        </activity>
        <activity android:name=".HospiterActivity"></activity>
        <activity android:name=".KnowledgeActivity"></activity>
        <activity android:name=".ShelterActivity"></activity>
        <activity android:name=".KnowledgeDataActivity"></activity>
    </application>

</manifest>