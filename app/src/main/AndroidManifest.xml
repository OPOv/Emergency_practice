    <meta-data android:name="com.kakao.sdk.AppKey" android:value="3c7b66994bc02c21a1129b00228d5ad5"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_image"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo_image_tr"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".SplashActivity"
            android:noHistory="true">
            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />


            </intent-filter>
        </activity>
        <activity android:name=".ShelterActivity" />
        <activity android:name=".KnowledgeDataActivity" />
        <activity android:name=".test"/>
        <activity android:name=".DMapView"/>
        <activity android:name=".NavigateActivity"/>
    </application>

</manifest>